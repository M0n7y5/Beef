{
  "name": "beeflang",
  "displayName": "Beef Lang",
  "description": "Adds support for the Beef programming language.",
  "version": "0.0.1",
  "publisher": "MineGame159",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:bf",
    "onCommand:beeflang.workspaceSettings"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "bf",
        "aliases": [
          "Beef",
          "bf"
        ],
        "extensions": [
          ".bf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "bf",
        "scopeName": "source.bf",
        "path": "./syntaxes/beef-old.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "beeflang.changeConfiguration",
        "title": "Change Configuration",
        "category": "Beef"
      },
      {
        "command": "beeflang.workspaceSettings",
        "title": "Open Workspace Settings",
        "category": "Beef"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c",
    "watch": "rollup -c -w"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.0.1",
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@types/glob": "^7.1.4",
    "@types/node": "14.x",
    "@types/vscode": "^1.70.0",
    "@yelo/rollup-node-external": "^1.0.1",
    "acorn-jsx": "^5.3.2",
    "glob": "^7.1.7",
    "js-yaml": "^4.1.0",
    "preact": "^10.10.6",
    "rollup": "^2.79.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
