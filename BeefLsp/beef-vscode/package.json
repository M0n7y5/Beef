{
  "name": "beeflang",
	"displayName": "Beef Lang",
	"description": "Adds support for the Beef programming language.",
	"version": "0.0.1",
	"publisher": "MineGame159",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:bf"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [{
            "id": "bf",
            "aliases": ["Beef", "bf"],
            "extensions": [".bf"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bf",
            "scopeName": "source.bf",
            "path": "./syntaxes/beef.tmLanguage"
        }]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"dependencies": {
		"vscode-languageclient": "^7.0.0"
	},
	"devDependencies": {
		"@types/vscode": "^1.70.0",
		"@types/glob": "^7.1.4",
		"@types/node": "14.x",
		"glob": "^7.1.7",
		"typescript": "^4.4.3"
	}
}
